{
  "name": "plop-generate-component",
  "version": "1.0.4",
  "description": "Библиотека для генерации компонентов с архитектурой FSD",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "files": [
    "dist",
    "plop-templates"
  ],
  "keywords": [
    "plop",
    "generator",
    "react",
    "component",
    "fsd",
    "feature-sliced",
