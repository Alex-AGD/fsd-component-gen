  "author": "Ваше имя",
  "license": "MIT",
  "scripts": {
    "build": "tsc && npm run build:cjs",
    "build:cjs": "node ./scripts/build-cjs.js",
    "prepare": "npm run build"
  },
  "dependencies": {
    "plop": "^4.0.1"
  },
  "devDependencies": {
    "typescript": "^5.5.3",
    "@types/node": "^20.11.0"
